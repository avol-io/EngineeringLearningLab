{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"],
    "lib": ["es2017", "dom"],
    "types": [
      "jasmine",
      "node",
      "core-js",
      "jquery",
      "lodash",
      "protractor"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
